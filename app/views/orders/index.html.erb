<h1>My Orders</h1>

<ul class="order-list">
  <% @orders.each do |order| %>
    <li>
      <strong>Order ID:</strong> <%= order.group_order.id %> |
      <strong>Product:</strong> <%= order.group_order.product.product_name %> |
      <strong>Quantity:</strong> <%= order.quantity_ordered %> |
      <strong>Status:</strong> <%= order.group_order.status %>
    </li>
  <% end %>
</ul>
